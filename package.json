{
  "name": "emmet-file",
  "displayName": "Emmet-Style File Create",
  "description": "Make multiple files, parent and child files & directories in an emmet-style syntax",
  "repository": {
    "type": "git",
    "url": "https://github.com/Crunchitect/emmet-file"
  },
  "version": "0.0.5",
  "publisher": "crunchi",
  "icon": "emmetfile.png",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "emmet-file.track",
        "title": "Track Files"
      }
    ],
    "configuration": {
      "title": "EmmetFile",
      "properties": {
        "emmetFile.templates": {
          "type": "object",
          "default": {
            "[html]": "index.html+style.css+main.js",
            "[py]": "main.py+requirements.txt",
            "[git]": "README.md+LICENSE.md+.gitignore"
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.3"
  }
}